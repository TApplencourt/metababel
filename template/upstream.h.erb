#pragma once
#include "component.h"
#ifdef __cplusplus
extern "C" {
#endif
<% dispatchers.each do |e| %>
<%# The signature type of callbacks %>
typedef void
<%= e.name_sanitized %>_callback_f(common_data_t *common_data, void *usr_data,
                                   <%= e.args.map{ |s| s.type }.join(", ") %>);
<%# The Function who register the callbacks to the dispatcher %>
extern void btx_register_callbacks_<%= e.name_sanitized %>(
    name_to_dispatcher_t **name_to_dispatcher,
    <%= e.name_sanitized %>_callback_f *callback);
<% end %>
#ifdef __cplusplus
}
#endif
